<h1>すべての投稿</h1>
 <% posts.each do |post| %>
 <% if post.image? %>
   <%= image_tag post.image.thumb.url, alt: '画像'%>
   <% else %>
      <%= video_tag post.video.url%>
   <% end %>
   <%= image_tag post.dog.cover_image.thumb.url %>
   <%= link_to public_post_path(post) do %>
   <%= post.dog.name %>
   <% end %>
   <%= post.title %>
   <%= post.text %>
   <%= render 'public/posts/tag_list', tag_list: post.tag_list %>
   <%= post.updated_at.strftime("%Y-%m-%d %H:%M") %>
 <% end %>
