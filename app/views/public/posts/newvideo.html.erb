<h1>動画投稿</h1>
  <%= form_for @post, url: public_posts_path do |f| %>
 	 <%= f.label :dog_id, '対象のペット' %>
 	   <%= f.collection_select :dog_id, @dogs, :id, :name, {prompt: "選択してください"} %>
     <%= f.label :video, '動画' %>
 	   <%= f.file_field :video, :accept => 'video/*' %>
 	 <%= f.label :title, 'タイトル' %>
 	   <%= f.text_field :title %>
     <%= f.label :text, '本文' %>
       <%= f.text_area :text %>
     <%= f.submit '投稿' %>
 <% end %>